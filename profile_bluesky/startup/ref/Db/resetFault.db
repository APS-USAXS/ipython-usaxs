record(bo, "$(P)$(R)sendPulse")
{
    field(DESC,"Reset AKD STO Fault")
    field(OUT, "$(P)$(BR)  PP NMS")
    field(VAL, "0")
    field(ZNAM,"Done")
    field(ONAM,"Reset")
    field(OMSL,"supervisory")
    field(HIGH,"1.0")
}

record(sseq, "$(P)$(R)resetFault")
{
    field(DESC,"AKD fault reset seq")
    field(STR1,"1")
    field(LNK1,"$(P)$(BS)  PP NMS")
    field(DLY2,"0.5")
    field(STR2,"1")
    field(LNK2,"$(P)$(R)sendPulse  PP NMS")
}
