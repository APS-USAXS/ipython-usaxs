grecord(longout,"$(P)tint")
{
	field(VAL,"0")
	field(DESC,"time interval")
	field(EGU, "sec")
}
grecord(ao,"$(P)cntTime")
{
	field(VAL,"0")
	field(DESC,"total counting time")
	field(EGU, "sec")
	field(PREC,"2")
}
grecord(longout,"$(P)Nring")
{
	field(VAL,"0")
	field(DESC,"size of ring buffer")
	field(EGU, "")
}
grecord(bo,"$(P)toggle")
{
	field(DESC,"testing only, not needed")
}
grecord(mbbo,"$(P)debug")
{
	field(VAL,"0")
	field(DESC,"debug value for dark")
	field(ZRST,"0")
	field(ONST,"1")
	field(TWST,"2")
	field(THST,"3")
	field(FRST,"4")
	field(FVST,"5")
	field(SXST,"6")
	field(SVST,"7")
	field(EIST,"8")
	field(NIST,"9")
	field(TEST,"10")
}
grecord(stringout,"$(P)version") {
      field(VAL, "")
}
grecord(bo,"$(P)reInit")
{
	field(VAL,"0")
	field(DESC,"init")
}
